{
  "compilerOptions": {
    "composite": true,
    "declaration": true,
    "declarationMap": false,
    // "lib": ["DOM", "ES2020"],
    "lib": ["ESNext", "DOM"],
    // "module": "CommonJS",
    "module": "esnext",
    "moduleResolution": "node",
    "newLine": "lf",
    "noEmitOnError": true,
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "strict": true,
    "strictNullChecks": true,
    "target": "ES2018",
    "baseUrl": "./",
    "paths": {
      "@temp/*": ["docs/.vuepress/.temp/*"]
    }
  },
  "include": ["**/.vuepress/**/*"],
  "exclude": ["node_modules", ".temp", "lib", "dist"]
}
